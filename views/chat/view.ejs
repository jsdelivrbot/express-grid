<nav class="breadcrumb pull-right boxy-small">
	<a class="breadcrumb-item" href="/">Home</a> /
	<a class="breadcrumb-item" href="/chat">Index</a> /
	<span class="breadcrumb-item active">View</span>
</nav>

<div class="page-header">
	<h1>Chat <%= data.model.id %> </h1>
</div>

<table class="table table-striped table-responsive boxy">
	<tbody>
		<tr>
			<th>
			<%= data.attributeData.message %>
			</th>
			<td>
			<%- data.model.message %>
			</td>
		</tr>
		<tr>
			<th>
			<%= data.attributeData.from %>
			</th>
			<td>
			<%- data.model.from %>
			</td>
		</tr>
		<tr>
			<th>
			<%= data.attributeData.to %>
			</th>
			<td>
			<%- data.model.to %>
			</td>
		</tr>
		<tr>
			<th>
			<%= data.attributeData.updatedAt %>
			</th>
			<td>
			<%- data.model.updatedAt %>
			</td>
		</tr>
		<tr>
			<th>
			<%= data.attributeData.id %>
			</th>
			<td>
			<%- data.model.id %>
			</td>
		</tr>
	</tbody>
</table>
