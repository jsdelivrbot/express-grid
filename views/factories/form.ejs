<form id="factories-form">
	<div class="form-group divname">
		<label for="name">Name</label>
		<input type="text" class="form-control" id="name" name="name" placeholder="name" value="<%= data.model.name%>">
	</div>
	<div class="form-group">
		<label for="address">Address</label>
		<input type="text" class="form-control" id="address" name="address" placeholder="address" value="<%= data.model.address%>">
	</div>
	<div class="form-group">
		<label for="city">City</label>
		<input type="text" class="form-control" id="city" name="city" placeholder="city" value="<%= data.model.city%>">
	</div>
	<div class="form-group">
		<label for="province">Province</label>
		<input type="text" class="form-control" id="province" name="province" placeholder="province" value="<%= data.model.province%>">
	</div>
	<div class="form-group">
		<label for="code">Code</label>
		<input type="text" class="form-control" id="code" name="code" placeholder="code" value="<%= data.model.code%>">
	</div>
	<div class="form-group">
		<label for="status">Status</label>
		<input type="text" class="form-control" id="status" name="status" placeholder="status" value="<%= data.model.status%>">
	</div>

 <div class="form-group"><button type="submit" id="submit-form" class="btn btn-success">Submit</button></div>
</form>

<% include formjs.ejs %>
